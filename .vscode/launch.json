{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Debug TypeScript",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node_modules/ts-node/dist/bin.js",
            "args": [
                "${workspaceFolder}/src/server.ts"
            ],
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack:///./src/*": "${workspaceFolder}/src/*"
            }
        }
    ]
  }